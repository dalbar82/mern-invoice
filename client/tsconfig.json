{
    "compilerOptions": {
        "types": ["react", "react-dom"],
        "lib": ["es6", "dom"],
        "jsx": "react-jsx",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true
      }

  }